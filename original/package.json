{
  "dependencies": {
    "@minecraft/common": "latest",
    "@minecraft/debug-utilities": "beta",
    "@minecraft/diagnostics": "beta",
    "@minecraft/math": "latest",
    "@minecraft/server": "beta",
    "@minecraft/server-admin": "beta",
    "@minecraft/server-editor": "beta",
    "@minecraft/server-gametest": "beta",
    "@minecraft/server-graphics": "beta",
    "@minecraft/server-net": "beta",
    "@minecraft/server-ui": "beta",
    "@minecraft/vanilla-data": "preview"
  },
  "overrides": {
    "@minecraft/debug-utilities": {
      "@minecraft/common": "$@minecraft/common"
    },
    "@minecraft/math": {
      "@minecraft/server": "$@minecraft/server"
    },
    "@minecraft/server": {
      "@minecraft/common": "$@minecraft/common",
      "@minecraft/vanilla-data": "$@minecraft/vanilla-data"
    },
    "@minecraft/server-admin": {
      "@minecraft/common": "$@minecraft/common"
    },
    "@minecraft/server-editor": {
      "@minecraft/common": "$@minecraft/common",
      "@minecraft/server": "$@minecraft/server"
    },
    "@minecraft/server-gametest": {
      "@minecraft/common": "$@minecraft/common",
      "@minecraft/server": "$@minecraft/server"
    },
    "@minecraft/server-net": {
      "@minecraft/common": "$@minecraft/common",
      "@minecraft/server-admin": "$@minecraft/server-admin"
    },
    "@minecraft/server-ui": {
      "@minecraft/common": "$@minecraft/common",
      "@minecraft/server": "$@minecraft/server"
    }
  }
}